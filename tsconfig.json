{
    "compilerOptions": {
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "module": "system",
        "moduleResolution": "node",
        "noEmitOnError": false,
        "noImplicitAny": false,
        "rootDir": "./",
        "outDir": "./",
        "removeComments": true,
        "sourceMap": true,
        "suppressImplicitAnyIndexErrors": true,
        "target": "es5",
        "watch": false
    },
    "exclude": [
        "./node_modules",
        "./typings/main.d.ts",
        "./typings/main"
    ]
}